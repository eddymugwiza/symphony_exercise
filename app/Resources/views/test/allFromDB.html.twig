    {%  extends 'base.html.twig' %}

    {% block body %}
        <table>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Last Name</th>
                <th>JMBg</th>
            </tr>
            {% for user in users %}
                <tr>
                        <td>{{ user.name }}</td>
                        <td>{{ user.lastName }}</td>
                        <td>{{ user.address }}</td>
                        <td>{{ user.jmbg }}</td>
                </tr>
            {% endfor %}
        </table>
    {% endblock %}

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('res/css/base.css') }}" />
    {% endblock %}

    {% block javascripts %}
        <script type = "text/javascript"
            src = "{{ asset('res/js/lib/jquery.js') }}"></script>
    {% endblock %}